				fetch1		macro
						local	fetch11,fetch12

						mov	step_counter,step_flg
						and	step_counter,step_counter
						jnz	fetch11

						mov	temp_h,ds:[PC80]
						inc	PC80
						PC_mask
						jmp	temp
				fetch11:
						dec	step_counter
						mov	step_flg,step_counter
						jz	fetch12

						mov	temp_h,ds:[PC80]
						inc	PC80
						PC_mask
						jmp	temp
				fetch12:
						mov	temp,reg_w
						mov	exit_code,code_step
						retf
				endm
